services:
  catalog.api:
    build:
      context: .
      dockerfile: src/Catalog/Catalog.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog.API
      target: base
    container_name: Catalog.API
    depends_on:
      catalogdb:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings:CatalogDatabaseSettings: Server=catalogdb,1433;Database=BasketDB;User
        Id=sa;Password=Pass@1234;
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 8000
      protocol: tcp
    - mode: ingress
      target: 443
      published: 8000
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\training\study\Microservices-NET6\MicroservicesAPP\src\Catalog\Catalog.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\AntoniusA\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\AntoniusA\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\AntoniusA\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
      target: /root/.nuget/fallbackpackages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft\Xamarin\NuGet\
      target: /root/.nuget/fallbackpackages2
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\AntoniusA\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: D:\training\study\Microservices-NET6\MicroservicesAPP
      target: /src
      bind:
        create_host_path: true
  catalogdb:
    container_name: catalogdb
    image: mcr.microsoft.com/mssql/server
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: 1433
      protocol: tcp
    restart: always
    volumes:
    - type: bind
      source: /site
      target: /data/db
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose17972507872940137663_default